{
  "name": "folder-structure",
  "displayName": "Folder Structure",
  "description": "Tools for Project Folder Structure or FR Feature Folder",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/dark.png",
  "repository": {"url": "https://github.com/armando-rodrigues/folder-structure.git"},
  "activationEvents": ["extension.createStructure", "extension.arquiveFolder"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createStructure",
        "title": "Create Folder Structure"
      },
      {
        "command": "extension.arquiveFolder",
        "title": "Arquive Folder Structure"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.createStructure",
          "group": "folder"
        },
        {
          "command": "extension.arquiveFolder",
          "group": "folder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
